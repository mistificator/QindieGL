    {
        "version": "2.0.0",
        "tasks": [
            {
                "label": "CMake: Configure x64",
                "type": "shell",
                "command": "cmake ${workspaceFolder} -B ${workspaceFolder}/build-x64 -A x64",
                "group": "build",
                "problemMatcher": []
            },
            {
                "label": "CMake: Configure x86",
                "type": "shell",
                "command": "cmake ${workspaceFolder} -B ${workspaceFolder}/build-x86 -A Win32",
                "group": "build",
                "problemMatcher": []
            },
            {
                "label": "CMake: Build x64",
                "type": "shell",
                "command": "cmake --build ${workspaceFolder}/build-x64 --config Release",
                "group": {
                    "kind": "build",
                    "isDefault": true
                },
                "problemMatcher": []
            },
            {
                "label": "CMake: Build x86",
                "type": "shell",
                "command": "cmake --build ${workspaceFolder}/build-x86 --config Debug",
                "group": {
                    "kind": "build",
                    "isDefault": false
                },
                "problemMatcher": []
            }
        ]
    }